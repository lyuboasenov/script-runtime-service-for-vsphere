{
  "Logging": {
    "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "RunspaceProviderSettings": {    
    "RunspaceContainerName": "pclirunspace",
    "RunspaceContainerNetworkName": "docker_gwbridge",
    "RunspaceContainerMemoryLimitMB": 512,
    "MaxNumberOfRunspaces": 5,
    "MaxRunspaceIdleTimeMinutes": 10,
    "MaxRunspaceActiveTimeMinutes": 60
  },
  "ScriptExecutionStorageSettings": {
    "HostScriptStorageDir": "/var/vmware/scriptruntimeservice/scriptsstore",
    "ServiceScriptStorageDir": "script_executions",
    "NumberOfScriptsPerUser": 30,
    "NoOlderThanDays": 5
  }
}
